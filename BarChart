import java.util.Scanner;

public class BarChart {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int artPoints, bobPoints, calPoints, danPoints, eliPoints;

        System.out.print("Enter the points scored by Art: ");
        artPoints = input.nextInt();
        System.out.print("Enter the points scored by Bob: ");
        bobPoints = input.nextInt();
        System.out.print("Enter the points scored by Cal: ");
        calPoints = input.nextInt();
        System.out.print("Enter the points scored by Dan: ");
        danPoints = input.nextInt();
        System.out.print("Enter the points scored by Eli: ");
        eliPoints = input.nextInt();

        System.out.println("Points Bar Chart:");
        System.out.print("Art   ");
        for (int i = 0; i < artPoints; i++) {
            System.out.print("*");
        }
        System.out.println();

        System.out.print("Bob   ");
        for (int i = 0; i < bobPoints; i++) {
            System.out.print("*");
        }
        System.out.println();

        System.out.print("Cal   ");
        for (int i = 0; i < calPoints; i++) {
            System.out.print("*");
        }
        System.out.println();

        System.out.print("Dan   ");
        for (int i = 0; i < danPoints; i++) {
            System.out.print("*");
        }
        System.out.println();

        System.out.print("Eli   ");
        for (int i = 0; i < eliPoints; i++) {
            System.out.print("*");
        }
        System.out.println();
    }
}
